{
  "name": "collision.js",
  "version": "1.1.0",
  "description": "Simple 2D JavaScript collision detection library",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./lib && ./node_modules/.bin/tsc",
    "build:watch": "./node_modules/.bin/tsc -w",
    "lint": "./node_modules/.bin/eslint . --ext .ts",
    "lint:fix": "./node_modules/.bin/eslint --fix ./src/**/*.*",
    "prebuild": "npm run lint && npm t",
    "test": "./node_modules/.bin/jest",
    "test:coverage": "./node_modules/.bin/jest --coverage",
    "test:watch": "./node_modules/.bin/jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jesselpalmer/collision.js.git"
  },
  "keywords": [
    "canvas",
    "HTML5",
    "games",
    "collision",
    "detection"
  ],
  "author": "Jesse Palmer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesselpalmer/collision.js/issues"
  },
  "engines": {
    "node": ">12.x.x"
  },
  "homepage": "https://github.com/jesselpalmer/collision.js",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
