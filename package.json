{
  "name": "collision.js",
  "version": "1.1.0",
  "description": "Simple 2D JavaScript collision detection library",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./lib && ./node_modules/.bin/tsc",
    "build:watch": "./node_modules/.bin/tsc -w",
    "lint": "./node_modules/.bin/eslint . --ext .ts",
    "lint:fix": "./node_modules/.bin/eslint --fix ./src/**/*.*",
    "prebuild": "npm run lint",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "test:coverage": "nyc -r lcov -e .ts -x \"*.spec.ts\" mocha -r ts-node/register src/**/*.spec.ts && nyc report",
    "test:watch": "mocha -r ts-node/register --watch src/**/*.spec.ts --watch-extensions ts"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jesselpalmer/collision.js.git"
  },
  "keywords": [
    "canvas",
    "HTML5",
    "games",
    "collision",
    "detection"
  ],
  "author": "Jesse Palmer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesselpalmer/collision.js/issues"
  },
  "engines": {
    "node": "10.x"
  },
  "homepage": "https://github.com/jesselpalmer/collision.js",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.4",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "chai": "^4.2.0",
    "eslint": "^7.11.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {}
}
