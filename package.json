{
  "name": "collision.js",
  "version": "1.1.0",
  "description": "Simple 2D JavaScript collision detection library",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile",
    "compile": "rm -rf ./lib && tsc",
    "compile:watch": "rm -rf ./lib && tsc -w",
    "clean": "gts clean",
    "fix": "gts fix",
    "lint": "gts lint",
    "prebuild": "npm run lint && npm t",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jesselpalmer/collision.js.git"
  },
  "keywords": [
    "canvas",
    "HTML5",
    "games",
    "collision",
    "detection"
  ],
  "author": "Jesse Palmer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesselpalmer/collision.js/issues"
  },
  "engines": {
    "node": ">12.x.x"
  },
  "homepage": "https://github.com/jesselpalmer/collision.js",
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "gts": "^3.1.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "10.9.1",
    "typescript": "^4.0.3"
  }
}
