{
  "name": "collision.js",
  "version": "1.1.0",
  "description": "Simple 2D JavaScript collision detection library",
  "main": "index.js",
  "scripts": {
    "compile": "rm -rf ./lib && tsc",
    "compile:watch": "rm -rf ./lib && tsc -w",
    "clean": "gts clean",
    "fix": "gts fix",
    "lint": "gts lint",
    "prebuild": "npm run lint && npm t",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jesselpalmer/collision.js.git"
  },
  "keywords": [
    "canvas",
    "HTML5",
    "games",
    "collision",
    "detection"
  ],
  "author": "Jesse Palmer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesselpalmer/collision.js/issues"
  },
  "engines": {
    "node": ">12.x.x"
  },
  "homepage": "https://github.com/jesselpalmer/collision.js",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "gts": "^3.1.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.3"
  }
}
