{
  "name": "collision.js",
  "version": "1.1.0",
  "description": "HTML5 collision game library.",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/.bin/tsc",
    "build:watch": "node_modules/.bin/tsc -w",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "test:coverage": "nyc -r lcov -e .ts -x \"*.spec.ts\" mocha -r ts-node/register src/**/*.spec.ts && nyc report"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jesselpalmer/collision.js.git"
  },
  "keywords": [
    "canvas",
    "HTML5",
    "games",
    "collision",
    "detection"
  ],
  "author": "Jesse Palmer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesselpalmer/collision.js/issues"
  },
  "homepage": "https://github.com/jesselpalmer/collision.js",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {}
}
